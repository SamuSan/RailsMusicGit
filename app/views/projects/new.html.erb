  <container class="col-md-12">
      <div class="main_container">
          <div class="project_nav_bar index_title">New Project</div>
          <table>
            <%= form_for Project.new do |f| %>
            <tr>
              <td class="col-md-3 index_project">
                Project Title:<%= f.text_field :title, autofocus: true %>
              </td>
            </tr>
            <tr>
              <td class="col-md-3 index_project">
                Tempo : <%= f.number_field :tempo %>  
              </td>
            </tr>
              <tr>
              <td class="col-md-3 index_project">
                <%= f.submit "Create" %>
              </td>
            </tr>
            <% end %>
          </table>     
      </div>
  </container>
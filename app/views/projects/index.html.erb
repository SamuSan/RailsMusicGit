<container class="col-md-6">
  <div class="main_container">
    <h1>Projects</h1>

      <% @projects.each do |project| %>
        <div>
          <label>Title : <%= link_to "#{project.title}", project %></label>
          <label>Tempo : <%= project.tempo %></label>
        </div>
      <% end %>

    <%= link_to "New Project", new_project_path%>
  </div>
</container>